<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="209dp"
    android:layout_height="232dp"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginLeft="12dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/background_bottom_rect">
    <ImageView
        android:id="@+id/proj_img_iv"
        android:layout_width="match_parent"
        android:layout_height="174dp"
        android:src="@color/colorPrimary"/>
    <TextView
        android:id="@+id/proj_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/proj_img_iv"
        android:layout_marginStart="11dp"
        android:layout_marginTop="1dp"
        android:textSize="14sp"
        android:textColor="#444444"
        android:fontFamily="@font/noto_b"
        tools:text="프로젝트 이름"/>
    <TextView
        android:id="@+id/proj_cate_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/proj_img_iv"
        android:layout_marginStart="11dp"
        android:layout_marginTop="22dp"
        android:textSize="13sp"
        android:textColor="#444444"
        android:fontFamily="@font/noto_r"
        tools:text="카테고리 이름"/>
</RelativeLayout>